{
  "manifest_version": 3,
  "name": "Cool Cookie tool",
  "description": "TMA project 2023",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "short_name": "CCT",
  "permissions": [
    "tabs",
    "cookies"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
"icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}